{
  "basic": {
    //刷的地区，空为不限
    //"area":"广东",
    //刷的点击次数，不填或填0表示不刷点击，目前针对电信，只点击页面中的img标签
    "imgClick":10,
    //需要投放的总UV数（广告主监测到的）
    "totaluv": 100,
    //Adobe监测到的PV和UV的比值
    "advPVAdvUV": 1.5,
    //DSP端增加的点击与Adobe监测的UV的比值
    "dspClickAdvUV": 1.6,
    //DSP端增加的曝光量和Adobe监测UV的比值
    "dspImpAdvUV": 700
    //例，以上4个参数分别为100, 1.4, 1.6, 700的时候:
    //会在Adobe上总共刷100个UV，100*1.5=150个PV
    //在我们DSP上刷100×1.6=160个点击，100*700=70000个展示
  },
  "url": [
    //要刷的广告主落地页，包括宏
    "http://www.turing.click/test/index.htm"
  ],
  "size": [
    //尺寸，分别替换{width}和{height}。URL中没有这个宏的时候，随意填一个值就行
    {
      "width": 640,
      "height": 100
    }
  ],
  "camp":[
    //此处每个记录对应一个渠道，在渠道内再设置这个渠道中所有需要刷的活动
    {
      //渠道ID，替换{adxid}，1为光音渠道，2为科大讯飞，URL中没有这个宏的时候，随意填一个值就行。
      "adxid":"10004",
      "campid":[
        //图灵DSP平台上，该渠道中的活动ID
        "248"
      ]
    }
  ],
  "ctid": [
    //创意包ID，替换{ctid}，URL中没有这个宏的时候，随意填一个值就行
    "53833"
  ],
  "spotid": [
    //广告位ID，替换{spotid}，URL中没有这个宏的时候，随意填一个值就行
    "sadf"
  ],
  "tag": [
    //人群标签值/ID，替换{tag}，URL中没有这个宏的时候，随意填一个值就行
    "81"
  ]
}
